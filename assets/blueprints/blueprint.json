{
    "landingPage": "/wp-admin/post-new.php?post_type=boostbox_popups",
    "preferredVersions": {
        "php": "8.0",
        "wp": "6.4.3"
    },
    "phpExtensionBundles": [
        "kitchen-sink"
    ],
    "steps": [
        {
            "step": "login",
            "username": "admin",
            "password": "password"
        },
        {
            "step": "installPlugin",
            "pluginZipFile": {
                "resource": "wordpress.org\/plugins",
                "slug": "boostbox"
            },
            "options": {
                "activate": true
            }
        },
        {
            "step": "installTheme",
            "themeZipFile": {
                "resource": "wordpress.org\/themes",
                "slug": "olliewp"
            }
        },
        {
            "step": "importFile",
            "file": {
                "resource": "url",
                "url": "https://boostbox.pro/boostbox-blueprint-playground-demo-content.xml"
            }
        }
    ]
}